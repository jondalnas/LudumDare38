package com.Jonas.LD38.Jobs;

import com.Jonas.LD38.Jobs.Actions.JobAction;

public class Job {
	public Object gotoObject;
	public JobAction action;
	public String name;
	
	Job(Object gotoObject, JobAction action, String name) {
		this.gotoObject = gotoObject;
		this.action = action;
		this.name = name;
	}
}
